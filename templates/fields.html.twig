{% block altcha_widget %}
    {% if enable %}
        <script async defer src="{{ js_path }}" type="module"></script>

        <altcha-widget 
            data-live-ignore="data-live-ignore"
            challengeurl="{{ path('huluti_altcha_challenge') }}"
            strings='{{ strings|json_encode }}'
            {% if floating %}floating{% endif %}
        ></altcha-widget>
    {% endif %}
{% endblock %}
